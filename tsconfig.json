{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "typeRoots": ["node_modules/@types", "src/types"],
    "types": ["jest"],
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@constants/*": ["src/constants/*"],
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@states/*": ["src/states/*"],
      "@services/*": ["src/services/*"],
      "@theme/*": ["src/theme/*"],
      "@helper/*": ["src/helper/*"],
      "@navigations/*": ["src/navigations/*"],
      "@screens/*": ["src/screens/*"]
    }
  }
}
